/* #Custom Styles
================================================== */
li { margin-bottom: 0px; }

#tab-1, #tab-2, #tab-3, #tab-4, {
    margin: 0 ; 
    padding: 0; 
    font-size: 0.6875em !important; 
    font-family: 'HelveticaNeue' !important; 
    color: #000000;
 }

div.details li, {
  padding: 0 ;
  margin: 0;
}
  
div[id^="as-container-"] {
  display: flex; 
  justify-content: center; 
  flex-align: center;
  align-content: center;
  align-self: center;
}

.sk-logo {
	visibility:hidden
}
  
#sk-no-sizing {
    display:inline-block;
    position:relative;
    background-color: transparent;
    cursor: pointer;
    border:0;
    padding:0;
    color:#AE0ADD;
  	text-transform: lowercase;
}
